{
  "name": "@storagestack/core",
  "version": "0.1.3",
  "description": "This package has the core functionality of the storage stack family",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "compile": "webpack",
    "compile-test": "webpack --config webpack.config.test.js",
    "tslint": "tslint --project src/tsconfig.json"
  },
  "keywords": [
    "blockstack",
    "storage",
    "transformation",
    "blockchain"
  ],
  "author": "Joris Hermans",
  "license": "ISC",
  "dependencies": {
    "core-js": "^2.5.5",
    "es6-promise": "^4.2.4",
    "minimatch": "^3.0.4",
    "route-parser": "0.0.5",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/core-js": "^0.9.46",
    "@types/jest": "^22.2.3",
    "awesome-typescript-loader": "^4.0.1",
    "jest": "^22.4.3",
    "lerna": "^2.9.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
